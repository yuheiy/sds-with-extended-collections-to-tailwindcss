{
  "name": "sds-with-extended-collections-to-tailwindcss",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node --run build:sd && pnpm run -r build",
    "build:sd": "node scripts/build-tailwind-tokens.ts && style-dictionary build --config sd.config.ts",
    "dev": "pnpm run -r --parallel dev"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.10.7",
    "es-toolkit": "^1.43.0",
    "map-obj": "^6.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "style-dictionary": "^5.1.3",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.9.3"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.11.1",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.28.0"
}
